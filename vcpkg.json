{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "tbb",
    "geos",
    "pybind11",
    {
      "name": "tiff",
      "features": [
        "jpeg",
        "lzma",
        "webp",
        "zip",
        "libdeflate",
        "lerc",
        "zstd"
      ]
    },
    {
      "name": "gdal",
      "features": [
        "default-features",
        "tools"
      ]
    }
  ]
}